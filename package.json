{
  "name": "less-watch-compiler",
  "version": "1.4.2",
  "description": "A command that watches folders(and subfolders) for file changes and automatically compile the less css files into css. This is a file system watcher and compiler.",
  "main": "less-watch-compiler.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "tests/test.sh",
    "postversion": "git push && git push --tags",
    "commit": "git-cz -n"
  },
  "repository": {
    "url": "https://github.com/jonycheung/deadsimple-less-watch-compiler.git",
    "type": "git"
  },
  "bin": {
    "less-watch-compiler": "./less-watch-compiler.js"
  },
  "dependencies": {
    "amdefine": ">= 0.1.0",
    "commander": "^2.9.0",
    "extend": ">= 2.0.0",
    "global": "^4.3.1",
    "less": "^2.7.1",
    "less-watch-compiler": "^1.2.3",
    "mocha": "^3.1.2",
    "shelljs": ">= 0.4.0"
  },
  "engine": "node 0.10.26",
  "devDependencies": {
    "cz-conventional-changelog": "^1.2.0",
    "mocha": "^3.1.2"
  },
  "preferGlobal": "true",
  "keywords": [
    "less",
    "lessc",
    "compiler",
    "file system",
    "watcher",
    "watch",
    "compile",
    "folder",
    "directory",
    "files",
    "css"
  ],
  "author": "Jony Cheung",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonycheung/deadsimple-less-watch-compiler/issues"
  },
  "homepage": "https://github.com/jonycheung/deadsimple-less-watch-compiler",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
